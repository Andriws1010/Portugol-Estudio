programa
{
	
	funcao inicio()
	{
		escreva("Olá Mundo")
		faça
	}
}
/* $$$ Portugol Studio $$$ 
 * 
 * Esta seção do arquivo guarda informações do Portugol Studio.
 * Você pode apagá-la se estiver utilizando outro editor.
 * 
 * @POSICAO-CURSOR = 56; 
 * @PONTOS-DE-PARADA = ;
 * @SIMBOLOS-INSPECIONADOS = ;
 * @FILTRO-ARVORE-TIPOS-DE-DADO = inteiro, real, logico, cadeia, caracter, vazio;
 * @FILTRO-ARVORE-TIPOS-DE-SIMBOLO = variavel, vetor, matriz, funcao;
 */
programa
{
	
	funcao inicio()
	{
	 	cadeia frutas[4]
	 	inteiro contador = 0
	 	
	 	frutas[0] = "maça"
	 	frutas[1] = "pera"
	 	frutas[2] = "uva"
	 	frutas[3] = "jaca"

	 	faca{
	 		
	 		escreva (frutas[contador]) + "\n")
	 		contador++
	 		
	 	}enquanto	(contador<=3)
	}
}
/* $$$ Portugol Studio $$$ 
 * 
 * Esta seção do arquivo guarda informações do Portugol Studio.
 * Você pode apagá-la se estiver utilizando outro editor.
 * 
 * @POSICAO-CURSOR = 187; 
 * @PONTOS-DE-PARADA = ;
 * @SIMBOLOS-INSPECIONADOS = ;
 * @FILTRO-ARVORE-TIPOS-DE-DADO = inteiro, real, logico, cadeia, caracter, vazio;
 * @FILTRO-ARVORE-TIPOS-DE-SIMBOLO = variavel, vetor, matriz, funcao;
 */
programa
{
	
	funcao inicio()
	{
		real nota1,nota02,not03,nota04,media
		cadeia aluno

		escreva("Digite o nome do aluno:")
		leia(aluno)
	escreva("O seu nome é:" + aluno)
	escreva("digite  nota1")
	leia(nota1)
	escreva("Digite nota2")
	leia(nota2)
	escreva("Digite nota3")
	leia(nota3)
	escreva("Digite nota4")
	leia(nota4)

	media= (nota+nota2+nota3+nota4)/4
		escreva("sua media é:" + media)

		se(media>=7) {
			escreva( "\n" + "Parabens!! voce foi aprovado") 
	}
	{
		senao("\n" + "infelizmente voce nao foi aprovado")
	}

	
	
	
	}
}
/* $$$ Portugol Studio $$$ 
 * 
 * Esta seção do arquivo guarda informações do Portugol Studio.
 * Você pode apagá-la se estiver utilizando outro editor.
 * 
 * @POSICAO-CURSOR = 524; 
 * @PONTOS-DE-PARADA = ;
 * @SIMBOLOS-INSPECIONADOS = ;
 * @FILTRO-ARVORE-TIPOS-DE-DADO = inteiro, real, logico, cadeia, caracter, vazio;
 * @FILTRO-ARVORE-TIPOS-DE-SIMBOLO = variavel, vetor, matriz, funcao;
 */
programa
{
	
	funcao inicio()
	{
		inteiro contador,limite,resultdo,tabuada

		 contador=0
		 limite=10

		 escreva("qual tabuda voce quer que eu resova?:")
		 leia(tabuada)
		 
		
		faca{
		
		resultado= tabuada * contador
		escreva( tabuada" X " + contador + " = " + resultado + "\n" )
			contador ++
			
		}enquanto (contador<=limite)
	}
}
